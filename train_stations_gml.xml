            <tn:trainLines>
                <tn:trainLine id="L1">
                    <tn:from>Dortmund</tn:from>
                    <tn:to>Osnabrück</tn:to>
                </tn:trainLine>
                <tn:trainLine id="L6">
                    <tn:from>Osnabrück</tn:from>
                    <tn:to>Bremen</tn:to>
                </tn:trainLine>
                <tn:trainLine id="L7">
                    <tn:from>Hannover</tn:from>
                    <tn:to>Osnabrück</tn:to>
